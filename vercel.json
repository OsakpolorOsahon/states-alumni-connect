{
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico|images|manifest.json|sw.js).*)",
      "destination": "/index.html"
    }
  ]
}